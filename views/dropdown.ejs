                        <% } %>
                    </select>
                    <input type='submit' id='query' name='button' value='Välj' />
                </form>
            </div>

            <div class="cell">
                <div class=caption>Mothåll</div>
                <form action="/mothall" method="post">
                    <select name="mothall" id="mothall">
                        <% for(var i=0; i < dropdownVals_mothall.length; i++) { %>
                        <option value="<%= dropdownVals_mothall[i] %>"><%= dropdownVals_mothall[i] %></option>
                        <% } %>
                    </select>
                    <input type='submit' id='query' name='button' value='Välj' />
                </form>
            </div>
        </div>
    </center>

</body>

</html>